{
  "name": "omnigame-art-cdn",
  "version": "0.0.1",
  "private": true,
  "description": "Static AI-generated, fan-made artwork for Omnigame. Not affiliated with or endorsed by any original IP holders.",
  "scripts": {
    "dev": "vite",
    "build": "npm run export:omnigame && npm run generate:exports-index && vite build",
    "build:skip-ingest": "vite build",
    "preview": "vite preview",
    "ingest:omnigame": "ts-node importers/ingest.ts",
    "ingest:import": "ts-node importers/omnigame-import.ts",
    "ingest:update-index": "ts-node importers/update-universe-index.ts",
    "export:omnigame": "ts-node src/exporters/omnigame/export.ts",
    "generate:exports-index": "ts-node scripts/generate-exports-index.ts"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "zod": "^3.22.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0"
  }
}
